pipeline{
    agent {node{'master'}}
        stages{
            stage('git'){
                agent{'master'}
                steps{
                    git branch: 'declarativepipeline', url: 'https://github.com/pkgangisetti/game-of-life.git'
                }
            }
            stage('compile'){
                agent{'master'}
                steps{
                    sh 'mvn clean package'
                }
            }
            
        }
}